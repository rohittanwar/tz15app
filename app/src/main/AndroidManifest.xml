<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.myapps.materialapplication" >

    <uses-permission android:name="android.permission.INTERNET" />

    <!-- <uses-permission android:name="android.permission.INTERNET"/> -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/tzlogo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterEvent"
            android:label="@string/title_activity_register_event"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myapps.materialapplication.MainActivity" />
        </activity>
        <activity
            android:name=".RegisterWorkshop"
            android:label="@string/title_activity_register_workshop"
            android:parentActivityName=".MainActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myapps.materialapplication.MainActivity" />
        </activity>
        <activity
            android:name=".WorkshopPaymentDetailsActivity"
            android:label="@string/title_activity_workshop_payment_details"
            android:parentActivityName=".RegisterWorkshop" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myapps.materialapplication.RegisterWorkshop" />
        </activity>
        <activity
            android:name=".EventConfirmationActivity"
            android:label="@string/title_activity_event_confirmation"
            android:parentActivityName=".RegisterEvent" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.myapps.materialapplication.RegisterEvent" />
        </activity>
    </application>

</manifest>
